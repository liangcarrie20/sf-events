<div>
<h1>All Events in San Francisco</h1>

<table style="width:50%">
  	<tr>
    	<th>Start Time</th>
    	<th>Event</th>
    	<th>Cost</th>
  	</tr>
  	<% @events.each do |event| %>
  	<tr>
    	<td><%= event.event_time %></td>
      <div class="see-info">

      <td> <a href="/events/<%= event.id %>"><%= event.title %></a></td>
      </div>

    	<td><%= event.price %></td>
  	</tr>
  	<% end %>
</table>

</div>
